        <section class="contact section">
            <div class="contact__bgimg"></div>

            <div class="contact__boxes">

                <form action="" class="form">

                    <h3 class="form__heading">Umów się na wizytę</h3>

                    <div class="form__group">
                        <label for="name" class="form__group-title">Imię i Nazwisko:</label>
                        <input type="text" id="name" class="form__group-input">
                    </div>

                    <div class="form__group form__group--left">
                        <label for="phone" class="form__group-title">Telefon:</label>
                        <input type="number" form="phone" class="form__group-input">
                    </div>

                    <div class="form__group  form__group--right">
                        <label for="email" class="form__group-title">E-mail:</label>
                        <input type="email" id="email" class="form__group-input" required>
                    </div>


                    <div class="form__group">
                        <label for="story" class="form__group-title">Wiadomość:</label>
                        <textarea id="story" class="form__group-textarea"> </textarea>

                    </div>

                    <div class="form__group form__group--checkbox">

                        <div class="form__group-checkbox">
                            <input type="checkbox" id="privacy-police" />
                            <label for="privacy-police" class="toggle"><span></span></label>
                        </div>

                        <label for="privacy-police" class="form__group-title">Akceptuję Politykę Prywatności</label>

                    </div>

                    <div class="form__group form__group--checkbox">

                        <div class="form__group-checkbox">
                            <input type="checkbox" id="newsletter" required>
                            <label for="newsletter" class="toggle"><span></span></label>
                        </div>

                        <label for="newsletter" class="form__group-title">Subskrypcja newslettera</label>

                    </div>


                    <input type="submit" value="Wyślij" class="form__btn">

                </form>




